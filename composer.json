{
    "name": "soarecostin/laravel-survey-forms",
    "description": "A package to dynamically build surveys in Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Costin Soare",
            "email": "soarecostin@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "5.6.* || 5.7.* || 5.8.* || 6.*.*",
        "illuminate/http": "5.6.* || 5.7.* || 5.8.* || 6.*.*",
        "illuminate/database": "5.6.* || 5.7.* || 5.8.* || 6.*.*",
        "astrotomic/laravel-translatable": "^11.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.3",
        "phpunit/phpunit": "^8.3",
        "orchestra/testbench": "^3.8.5 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "SoareCostin\\LaravelSurveyForms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SoareCostin\\LaravelSurveyForms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoareCostin\\LaravelSurveyForms\\LaravelSurveyFormsServiceProvider"
            ]
        }
    }
}
{
    "name": "soarecostin/survey-builder",
    "description": "A package to dynamically build surveys in Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Costin Soare",
            "email": "soarecostin@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "5.6.* || 5.7.* || 5.8.* || 6.*.*",
        "illuminate/http": "5.6.* || 5.7.* || 5.8.* || 6.*.*",
        "astrotomic/laravel-translatable": "^11.3",
        "spatie/laravel-view-models": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "SoareCostin\\SurveyBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SoareCostin\\SurveyBuilder\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoareCostin\\SurveyBuilder\\SurveyBuilderServiceProvider"
            ]
        }
    }
}